package JavaBasico2;

public class ForParaOuImpar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 5;
		for (int i=0; i<=50; i++) {
			if ( ++x == x/2 ) {
				System.out.println(x + " É par!");
				continue;
			}
			//*if ( (3* x) + 1 == i ) {
				//System.out.println(i + "É ímpar!");
				//
		//}

}
}
}