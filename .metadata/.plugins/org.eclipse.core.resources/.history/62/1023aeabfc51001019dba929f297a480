package JavaBasico2;

public class ForParaOuImpar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 0;
		for (int i=0; i<=50; i++) {
			if (++x == x/2) {
				++x;
				System.out.println(x + " É par!");
				
			}
			//*if ( (3* x) + 1 == i ) {
				//System.out.println(i + "É ímpar!");
				//
		//}

}
}
}