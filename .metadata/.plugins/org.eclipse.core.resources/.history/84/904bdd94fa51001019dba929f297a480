package JavaBasico2;

public class ForParaOuImpar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 0;
		for (int i=0; i<=50; i++) {
			if ( x / 2 == 0) {
				System.out.println(i + " É par!");
				continue;
			}
			if ( 3* i + 1 == x ) {
				System.out.println(i + "É ímpar!");
				continue;
			}

}
}
}